datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         Int         @id @default(autoincrement())
  userName   String
  email      String
}

model Profile {
  id         Int         @id @default(autoincrement())
  firstName  String
  lastName   String
}